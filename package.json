{
  "name": "talkback-cli",
  "version": "1.0.0",
  "description": "Voice notifications for AI coding assistants - text-to-speech CLI with multi-provider support",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "talkback": "./dist/index.js",
    "talkback-mcp": "./dist/mcp-server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TalkbackAi/talkback.git"
  },
  "author": "",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "keywords": [
    "tts",
    "text-to-speech",
    "elevenlabs",
    "openai",
    "cli",
    "voice",
    "speech",
    "ai",
    "coding-assistant",
    "claude",
    "cursor",
    "aider",
    "mcp",
    "notifications"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^20.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.8.1",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.55.0",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.1",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "commander": "^14.0.3",
    "zod": "^3.25.76"
  }
}
